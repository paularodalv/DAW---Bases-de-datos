create database if not exists dos_siete;
use dos_siete;

create table if not exists pais(
	nombre varchar(30) primary key,
    num_habitantes varchar(30) not null
);

create table if not exists presidente(
	dni varchar(30) primary key,
    nombre varchar(30),
    apellido varchar(30),
    fecha_nac date not null,
    nombre_pais varchar(30) unique not null,
    foreign key (nombre_pais) references pais(nombre)
);
